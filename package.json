{
  "name": "rms-db",
  "version": "1.0.0",
  "description": "RMS Database Service - Node.js migration from Go",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "proto:generate": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=src/generated --proto_path=proto proto/backend_api.proto"
  },
  "keywords": ["rms", "grpc", "mongodb"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "mongodb": "^6.3.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "bcrypt": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/mongodb": "^4.0.7",
    "@types/bcrypt": "^5.0.2",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-protoc-gen": "^0.15.0"
  }
}

